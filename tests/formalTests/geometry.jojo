
class point {
	@@@
		public int x, y;
	@@@

	func public real max(int x1, int x2) {
		if (x1 < x2) {
			zadust x1;
		}
		zadust x2;
	}

	func real min(int x1, int x2) {
		if (x1 < x2) {
			zadust x1;
		}
		zadust x2;
	}

	func int abs(int x) {
		if (x < 0) {
			zadust -x;
		}
		zadust x;
	}

	func int dist2(int xx, int yy) {
		zadust 
			abs(xx - x) * abs(xx - x) + 
			abs(yy - y) * abs(yy - y);
	}
}

class point3d hamon point {
	@@@
		int z;
	@@@

	func real dist2(int xx, int yy, int zz) {
		zadust 
			abs(xx - x) * abs(xx - x) + 
			abs(yy - y) * abs(yy - y) +
			abs(zz - z) * abs(zz - z);
	}
}

