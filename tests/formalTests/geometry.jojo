
class point {
	@@@
		public int x, y;
	@@@

	func public int max(int x1, int x2) {
		if (x1 < x2) {
			zadust x1;
		}
		zadust x2;
	}

	func int min(int x1, int x2) {
		if (x1 < x2) {
			zadust x1;
		}
		zadust x2;
	}

	func int abs(int x) {
		if (x < 0) {
			zadust -x;
		}
		zadust x;
	}

	func int dist2(int xx, int yy) {
		zadust 
			zawarudo abs(xx - x) * zawarudo abs(xx - x) + 
			zawarudo abs(yy - y) * zawarudo abs(yy - y);
	}
}

class point3d hamon point {
	@@@
		int z;
	@@@

	func int dist2(int xx, int yy, int zz) {
		zadust 
			zawarudo abs(xx - x) * zawarudo abs(xx - x) + 
			zawarudo abs(yy - y) * zawarudo abs(yy - y) +
			zawarudo abs(zz - z) * zawarudo abs(zz - z);
	}
}

class line {
	@@@
		int a, b, c;
	@@@

	func public int distEntrePuntos() {
		@@@
			point p1;
			point p2;
		@@@
		zadust zawarudo p1.dist2(p2.x, p2.y);
	}
}

class line3d hamon line{
	func public int distEntrePuntos() {
		@@@
			point3d p1, p2, p3;
		@@@
		zadust zawarudo p1.dist2(p2.x, p2.y, p3.z);
	}
}



public stand jojo() {
	@@@
		point p1, p2;
		point3d p3, p4;
		line l1;
		line3d l2;
	@@@

	p1.x = 0;
	p1.y = 0;
