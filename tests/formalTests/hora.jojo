
class hora {
	@@@
		int h, m;
	@@@
	
	func public stand hhora(int hh, int mm) {
		h = hh;
		m = mm;
	}

	func public stand setH(int hh) {
		h = hh;
	}

	func public int getH() {
		zadust h;
	}

	func public stand pprints() {
		prints(h);
		prints(m);
	}
}


public stand jojo() {
	@@@
		hora h1, h2;
	@@@

	gets(h1.h);
	gets(h1.m);
	zawarudo h1.pprints();

	zawarudo h2.hhora(10, 42);
	zawarudo h2.pprints();

	zadust;
}