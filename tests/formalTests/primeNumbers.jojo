@@@
	int MAXN;
	int primes[100], f[100], pc;
@@@

func stand EratosthenesON() {
	@@@
		int i, j, aux2;
	@@@

	i = 2;
	pc = 0;
	while (i < MAXN) {
		if (f[i] == 0) {
			primes[pc] = i;
			f[i] = i;
			pc = pc + 1;
		}

		j = 0;
		while (j < pc && 
				i * primes[j] 
				< MAXN && 
				primes[j] <= f[i]) {
			aux2 = i*primes[j];
			f[aux2] = primes[j];
			prints(f[aux2]);
			j = j + 1;
		}            
		i = i + 1;
	}
}


